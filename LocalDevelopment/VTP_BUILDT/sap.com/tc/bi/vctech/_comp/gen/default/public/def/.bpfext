<technologies version="1">
  <technology name="sap.com/tc/bi/vctech">
    <uses>sap.com/tc/bi/wdtech</uses>
    <macrocontextprovider>
      <provider
          classname="com.sap.tc.buildplugin.techdev.vcmodel.CreateBuildFileAction"/>
    </macrocontextprovider>
    <generators>
      <generator type="sap.com~vcgen"
        implementation-type="java"
        implementation-name="com.sap.tc.buildplugin.gen.VisualComposerModelGenerator">
        <inputs>
          <input name="default" required="true" default-value="$dc_packageDirsAsFiles">
            <description>
              Source paths containing VC metamodel data.
            </description>
          </input>
        </inputs>
        <outputs>
          <output name="webdynpro"/>
          <output name="portal"/>
          <output name="support"/>
          <output name="modules"/>
        </outputs>
        <usedpaths>
          <path name="classpath" default-value="$vc_addPath_dep"/>
        </usedpaths>
        <parameters>
          <parameter name="vendor"   default-value="$dc_vendor"/>
          <parameter name="dcname"   default-value="$dc_name"/>
          <parameter name="scvendor" default-value="$dc_scvendor"/>
          <parameter name="scname"   default-value="$dc_scname"/>
        </parameters>
      </generator>
      <generator type="sap.com~vccheck"
        implementation-type="java"
        implementation-name="com.sap.tc.buildplugin.gen.VisualComposerModelCheckGenerator">
        <inputs>
          <input name="default" required="true" default-value="$dc_packageDirsAsFiles"/>
        </inputs>
        <outputs>
          <output name="logpath"/>
        </outputs>
        <usedpaths>
          <path name="classpath" default-value="$vc_addPath_dep"/>
        </usedpaths>
        <parameters>
          <parameter name="vendor"   default-value="$dc_vendor"/>
          <parameter name="dcname"   default-value="$dc_name"/>
          <parameter name="scvendor" default-value="$dc_scvendor"/>
          <parameter name="scname"   default-value="$dc_scname"/>
        </parameters>
      </generator>
    </generators>
    <buildfile-creators>
      <library
        implementation-name="com.sap.tc.buildplugin.VisualComposerBase"
        interface-name="com.sap.tc.buildplugin.IVisualComposerBase"
        context-name="visualComposerBase"/>
    </buildfile-creators>
    <ant-tasks>
      <task name="packpga" classname="com.sap.portal.generator.ant.taskdefs.PgaArchiverTask" on-demand="true"/>
    </ant-tasks>
  </technology>
</technologies>
