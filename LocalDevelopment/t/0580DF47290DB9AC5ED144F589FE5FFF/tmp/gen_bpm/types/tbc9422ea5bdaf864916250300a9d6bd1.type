CHILD_0,SCHEMA=<?xml version="1.0" encoding="UTF-8"?> <xsd:schema targetNamespace="sap.com/glx/" xmlns:tns="sap.com/glx/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">   <xsd:complexType name="TaskAttributes" sdox:aliasName="#TaskAttributes" xmlns:sdox="commonj.sdo/xml">     <xsd:sequence>       <xsd:element name="priority" type="tns:Priority" default="3" sdox:aliasName="$priority">         <xsd:annotation>           <xsd:documentation>     					Sets priority of the task instances     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="activationTime" type="xsd:dateTime" sdox:aliasName="$activationTime">         <xsd:annotation>           <xsd:documentation>     					Determines time when task will be activated     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="startDeadline" type="xsd:dateTime" sdox:aliasName="$startDeadline">         <xsd:annotation>           <xsd:documentation>     					Determines deadline when task should be started     					the latest     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="completionDeadline" type="xsd:dateTime" sdox:aliasName="$completionDeadline">         <xsd:annotation>           <xsd:documentation>     					Determines deadline when task should be     					completed the latest     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="expirationTime" type="xsd:dateTime" sdox:aliasName="$expirationTime">         <xsd:annotation>           <xsd:documentation>     					Determines time when task will expire (i.e. task     					client is no longer interested in task result)     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="createdOn" type="xsd:dateTime" sdox:aliasName="$createdOn">         <xsd:annotation>           <xsd:documentation>TODO</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="activatedOn" type="xsd:dateTime" sdox:aliasName="$activatedOn">         <xsd:annotation>           <xsd:documentation>     					Time when the task has been activated     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="startedOn" type="xsd:dateTime" sdox:aliasName="$startedOn">         <xsd:annotation>           <xsd:documentation>TODO</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="completedOn" type="xsd:dateTime" sdox:aliasName="$completedOn">         <xsd:annotation>           <xsd:documentation>TODO</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="startDeadlineIsCritical" type="xsd:boolean" sdox:aliasName="$startDeadlineIsCritical">         <xsd:annotation>           <xsd:documentation>     					This Boolean flag indicates that the Start     					Deadline is a critical one. If this attribute is     					set to true the Task is frozen and transitions     					into an error state.     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="completionDeadlineIsCritical" type="xsd:boolean" sdox:aliasName="$completionDeadlineIsCritical">         <xsd:annotation>           <xsd:documentation>     					This Boolean flag indicates that the Completion     					Deadline is a critical one. If this attribute is     					set to true the Task is frozen and transitions     					into an error state.     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="activity" type="xsd:string" sdox:aliasName="$activity">         <xsd:annotation>           <xsd:documentation>     					Temporary field to map the MOF Id of the referencing     					Human activity into the Task attributes.     				</xsd:documentation>         </xsd:annotation>       </xsd:element>       <xsd:element name="actualOwner" type="tns:Principal" sdox:aliasName="$actualOwner"></xsd:element>       <xsd:element name="initiator" type="tns:Principal" sdox:aliasName="$initiator"></xsd:element>       <xsd:element name="customAction" type="xsd:string" sdox:aliasName="$customAction">         <xsd:annotation>           <xsd:documentation>     					Field to set the custom action chosen to complete     					the task instance     				</xsd:documentation>         </xsd:annotation>       </xsd:element>     </xsd:sequence>   </xsd:complexType>   <xsd:simpleType name="Priority" sdox:aliasName="#Priority" xmlns:sdox="commonj.sdo/xml">     <xsd:restriction base="xsd:int">       <xsd:enumeration value="1">         <xsd:annotation>           <xsd:documentation>     				very high     				</xsd:documentation>         </xsd:annotation>       </xsd:enumeration>       <xsd:enumeration value="2">         <xsd:annotation>           <xsd:documentation>     				high     				</xsd:documentation>         </xsd:annotation>       </xsd:enumeration>       <xsd:enumeration value="3">         <xsd:annotation>           <xsd:documentation>     				medium     				</xsd:documentation>         </xsd:annotation>       </xsd:enumeration>       <xsd:enumeration value="4">         <xsd:annotation>           <xsd:documentation>     				low     				</xsd:documentation>         </xsd:annotation>       </xsd:enumeration>     </xsd:restriction>   </xsd:simpleType>   <xsd:complexType name="Principal" sdox:aliasName="#Principal" xmlns:sdox="commonj.sdo/xml">     <xsd:sequence>       <xsd:element name="principalId" type="xsd:string" sdox:aliasName="$principalId"></xsd:element>       <xsd:element name="type" type="tns:PrincipalType" default="1" sdox:aliasName="$type"></xsd:element>     </xsd:sequence>   </xsd:complexType>   <xsd:simpleType name="PrincipalType" sdox:aliasName="#PrincipalType" xmlns:sdox="commonj.sdo/xml">     <xsd:restriction base="xsd:int">       <xsd:enumeration value="1"></xsd:enumeration>       <xsd:enumeration value="2"></xsd:enumeration>       <xsd:enumeration value="3"></xsd:enumeration>     </xsd:restriction>   </xsd:simpleType> </xsd:schema> 

